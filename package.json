{
  "name": "rabbithq",
  "description": "RabbitMQ Dashboard Monitoring Tool",
  "private": true,
  "author": "La  Griffe",
  "license": "ISC",
  "keywords": [
    "rabbitmq",
    "dashboard",
    "monitoring"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/LaGriffe/rabbit-dash-board.git"
  },
  "workspaces": [
    "front-end",
    "back-end"
  ],
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev:front": "npm run dev --workspace=front-end",
    "dev:back": "npm run dev --workspace=back-end",
    "build": "npm run build --workspace=back-end",
    "start": "npm run start --workspace=back-end",
    "seed:connections": "npx tsx scripts/seed-connections.ts",
    "seed:messages": "npx tsx scripts/seed-messages.ts --skip-setup",
    "seed:alerts": "npx tsx scripts/seed-alerts.ts",
    "seed:alerts:cleanup": "tsx scripts/seed-alerts.ts cleanup",
    "generate:test-messages": "npx tsx scripts/generate-test-messages.ts",
    "test:server-management": "node scripts/test-server-management.js"
  }
}
