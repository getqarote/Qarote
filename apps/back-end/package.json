{
  "name": "back-end",
  "version": "1.0.0",
  "description": "Hono.js API for RabbitMQ Dashboard",
  "main": "dist/index.js",
  "keywords": [],
  "author": "La Griffe",
  "license": "ISC",
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:alert": "tsx watch src/workers/alert-monitor.ts",
    "build": "prisma generate && tsc && tsc-alias",
    "start": "node --require tsconfig-paths/register dist/index.js",
    "start:alert": "node --require tsconfig-paths/register dist/workers/alert-monitor.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "postinstall": "prisma generate",
    "build:production": "npm run db:generate && npm run build",
    "start:production": "npm run db:migrate && npm run start",
    "seed:test": "tsx scripts/rabbitmq/seed-test-data.ts",
    "seed:users": "tsx scripts/rabbitmq/seed-test-data.ts --type=users",
    "seed:limits": "tsx scripts/rabbitmq/seed-test-data.ts --type=limits",
    "seed:email": "tsx scripts/rabbitmq/seed-test-data.ts --type=email",
    "seed:clean": "tsx scripts/rabbitmq/seed-test-data.ts --type=clean",
    "seed:publish": "tsx scripts/rabbitmq/seed-publish-messages.ts",
    "seed:consume": "tsx scripts/rabbitmq/seed-consume-messages.ts",
    "seed:all": "tsx scripts/rabbitmq/seed-all-servers.ts",
    "seed:email:send": "tsx scripts/resend/seed-email-verification-tests.ts",
    "email:dev": "email dev --dir ./scripts/resend/emails --port 3001",
    "email:preview": "cd scripts/resend && email preview",
    "email:export": "cd scripts/resend && email export",
    "email:test": "tsx scripts/resend/test-email-rendering.ts",
    "email:test-cid": "tsx scripts/test-email-cid.ts",
    "webhook:test": "tsx scripts/webhook/test-webhook.ts",
    "stripe:set-up": "tsx scripts/stripe/setup-stripe.ts",
    "stripe:create": "tsx scripts/stripe/setup-stripe.ts create",
    "stripe:migrate": "tsx scripts/stripe/setup-stripe.ts migrate",
    "stripe:list": "tsx scripts/stripe/setup-stripe.ts list",
    "stripe:clean": "tsx scripts/stripe/setup-stripe.ts clean",
    "stripe:verify": "tsx scripts/stripe/setup-stripe.ts verify",
    "stripe:webhook": "tsx scripts/stripe/setup-stripe.ts webhook",
    "stripe:mode": "tsx scripts/stripe/setup-stripe.ts mode",
    "stripe:test": "tsx scripts/stripe/setup-stripe.ts test",
    "stripe:login": "stripe login",
    "webhook:secret": "stripe listen --print-secret",
    "notion:sync": "tsx scripts/notion/sync-users.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.4",
    "@hono/zod-validator": "^0.7.0",
    "@notionhq/client": "^5.3.0",
    "@prisma/client": "^6.19.0",
    "@react-email/button": "^0.1.0",
    "@react-email/components": "^0.1.0",
    "@react-email/container": "^0.0.15",
    "@react-email/heading": "^0.0.15",
    "@react-email/hr": "^0.0.11",
    "@react-email/html": "^0.0.11",
    "@react-email/img": "^0.0.11",
    "@react-email/preview": "^0.0.13",
    "@react-email/render": "^1.1.2",
    "@react-email/section": "^0.0.16",
    "@react-email/text": "^0.1.5",
    "@sentry/node": "^10.24.0",
    "@sentry/profiling-node": "^10.24.0",
    "@types/react": "^19.1.8",
    "@types/uuid": "^10.0.0",
    "amqplib": "^0.10.8",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "google-auth-library": "^10.3.0",
    "hono": "^4.10.8",
    "hono-rate-limiter": "^0.4.2",
    "is-valid-hostname": "^1.0.2",
    "nodemailer": "^7.0.11",
    "pg": "^8.16.3",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "react-email": "^4.2.8",
    "resend": "^4.6.0",
    "stripe": "^17.7.0",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@react-email/preview-server": "^5.0.7",
    "@types/amqplib": "^0.10.7",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/node": "^24.0.0",
    "@types/nodemailer": "^6.4.14",
    "eslint": "^9.28.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.15.0",
    "next": "^16.0.10",
    "prettier": "^3.6.0",
    "prisma": "^6.19.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vitest": "^4.0.15"
  }
}
